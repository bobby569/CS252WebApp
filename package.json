{
	"name": "Purdue2048",
	"version": "1.0.0",
	"description": "2048 Game in Purdue CS Version",
	"main": "index.js",
	"engines": {
		"node": "8.9.4",
		"npm": "5.6.0"
	},
	"scripts": {
		"setup": "yarn install && cd client && yarn install",
		"start": "node index.js",
		"server": "nodemon index.js",
		"client": "cd client && yarn run start",
		"dev": "concurrently \"yarn run server\" \"yarn run client\"",
		"heroku-postbuild":
			"NPM_CONFIG_PRODUCTION=false cd client && yarn install && yarn run build"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/bobby569/Purdue2048.git"
	},
	"keywords": ["Purdue", "CS", "Lawson", "2048", "Tracks"],
	"license": "MIT",
	"homepage": "https://github.com/bobby569/Purdue2048",
	"dependencies": {
		"bluebird": "^3.5.1",
		"body-parser": "^1.18.2",
		"concurrently": "^3.5.1",
		"cookie-session": "^2.0.0-beta.3",
		"express": "^4.16.2",
		"mongoose": "5.0.1",
		"passport": "^0.4.0",
		"passport-google-oauth20": "^1.0.0"
	},
	"devDependencies": {
		"nodemon": "1.14.11"
	}
}
